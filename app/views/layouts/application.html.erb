<!DOCTYPE html>
<html>
<head>
  <title>SessionManagement</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% if @app_session %>
  <p>Last pages visited:</p>
  <ul>
    <% @app_session.each do |app_session| %>
      <li><%= link_to app_session, app_session %></li>
    <% end %>
  </ul>
<% end %>


<%= yield %>

</body>
</html>
